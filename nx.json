{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "ngx-ui",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  },
  "projects": {
    "docs": {
      "tags": ["scope:docs", "type:app"]
    },
    "docs-e2e": {
      "tags": [],
      "implicitDependencies": ["docs"]
    },
    "ngx-doc": {
      "tags": ["scope:ngx-doc", "type:lib"]
    },
    "ngx-ui": {
      "tags": ["scope:ngx-ui", "type:lib"]
    },
    "ngx-ui-animations": {
      "tags": ["scope:animations", "type:lib"]
    },
    "ngx-ui-autosize-input": {
      "tags": ["scope:autosize-input", "type:lib"]
    },
    "ngx-ui-button": {
      "tags": ["scope:button", "type:lib"]
    },
    "ngx-ui-calendar": {
      "tags": ["scope:calendar", "type:lib"]
    },
    "ngx-ui-camel-to-snake": {
      "tags": ["scope:camel-to-snake", "type:lib"]
    },
    "ngx-ui-card": {
      "tags": ["scope:card", "type:lib"]
    },
    "ngx-ui-checkbox": {
      "tags": ["scope:checkbox", "type:lib"]
    },
    "ngx-ui-code-editor": {
      "tags": ["scope:code-editor", "type:lib"]
    },
    "ngx-ui-common": {
      "tags": ["scope:common", "type:lib"]
    },
    "ngx-ui-copy-to-clipboard": {
      "tags": ["scope:copy-to-clipboard", "type:lib"]
    },
    "ngx-ui-decamelize": {
      "tags": ["scope:decamelize", "type:lib"]
    },
    "ngx-ui-file-button": {
      "tags": ["scope:file-button", "type:lib"]
    },
    "ngx-ui-filter-by": {
      "tags": ["scope:filter-by", "type:lib"]
    },
    "ngx-ui-intersection-observer": {
      "tags": ["scope:intersection-observer", "type:lib"]
    },
    "ngx-ui-json-tree": {
      "tags": ["scope:json-tree", "type:lib"]
    },
    "ngx-ui-long-press": {
      "tags": ["scope:long-press", "type:lib"]
    },
    "ngx-ui-resize-observer": {
      "tags": ["scope:resize-observer", "type:lib"]
    },
    "ngx-ui-time-zone": {
      "tags": ["scope:time-zone", "type:lib"]
    },
    "ngx-ui-tooltip": {
      "tags": ["scope:tooltip", "type:lib"]
    },
    "ngx-ui-typings": {
      "tags": ["scope:typings", "type:lib"]
    },
    "ngx-ui-utils": {
      "tags": ["scope:utils", "type:lib"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
