{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "ngx-ui",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  },
  "projects": {
    "ngx-ui": {
      "tags": ["scope:ngx-ui", "type:library"]
    },
    "ngx-ui-typings": {
      "tags": ["scope:typings", "type:lib"]
    },
    "ngx-ui-utils": {
      "tags": ["scope:utils", "type:lib"]
    },
    "ngx-ui-common": {
      "tags": ["scope:common", "type:lib"]
    }
  }
}
