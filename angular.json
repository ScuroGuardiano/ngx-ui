{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {}
  },
  "projects": {
    "ngx-ui": {
      "projectType": "library",
      "root": "packages/ngx-ui",
      "sourceRoot": "packages/ngx-ui/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/packages/ngx-ui"],
          "options": {
            "project": "packages/ngx-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/ngx-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/ngx-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/ngx-ui"],
          "options": {
            "jestConfig": "packages/ngx-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/ngx-ui/src/**/*.ts",
              "packages/ngx-ui/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngx-ui"
}
