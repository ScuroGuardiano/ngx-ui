    "@types/json-schema": "7.0.7",
    "@types/mousetrap": "1.6.5",
    "@types/node": "14.14.33",
    "@types/prismjs": "1.16.3",
    "@types/tinycolor2": "1.4.2",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "autoprefixer": "10.2.5",
    "clean-css": "5.1.1",
    "commitizen": "4.2.3",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "8.2.0",
