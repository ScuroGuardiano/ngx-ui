    "ts-node": "~9.1.1",
    "typescript": "~4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts,html,scss,css}": [
      "npm run format:write"
    ]
  }
}
