    "@angular/cli": "11.2.4",
    "@angular/compiler-cli": "11.2.5",
    "@angular/language-service": "11.2.5",
    "@nrwl/angular": "11.4.0",
    "@nrwl/cli": "11.4.0",
    "@nrwl/eslint-plugin-nx": "11.4.0",
    "@nrwl/jest": "11.4.0",
    "@nrwl/linter": "11.4.0",
    "@nrwl/tao": "11.4.0",
    "@nrwl/workspace": "11.4.0",
    "@release-it/bumper": "2.0.0",
